public bool Boing(int notenum = 0)
{
    bool boinged = false;

    // Print("boing");
    if (notenum == 0)
    {
        notenum = rand.Next(30, 80);
        boinged = true;
    }

    SendNote("synth", notenum, VOL, DUR);
    
    return boinged;
}
