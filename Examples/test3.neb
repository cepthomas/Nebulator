///// Variables /////
COL1 = var(200); // change color
MODN = var(0); // modulate notes
PITCH = var(8192); // center is 8192

///// Realtime Controllers /////
MI1 = midiin(1, 4, MODN);
MO1 = midiout(1, Pitch, PITCH);

////// Levers //////
L1 = lever(0, 255, COL1);
